{
	"name": "yaml-plus-json",
	"displayName": "YAML plus JSON",
	"description": "Easily convert yaml to json and vice versa",
	"version": "0.0.1",
	"publisher": "hilleer",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/**.json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"@types/yaml": "1.2.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"i": "0.3.6",
		"npm": "6.13.7",
		"yaml": "1.7.2"
	},
	"bugs": {
		"url": "https://github.com/hilleer/vscode-yaml-plus-json/issues/new"
	},
	"repository": {
		"url": "https://github.com/hilleer/vscode-yaml-plus-json",
		"type": "git"
	}
}
