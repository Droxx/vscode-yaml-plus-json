{
	"name": "yaml-plus-json",
	"displayName": "YAML ❤️ JSON",
	"description": "Easily convert yaml to json and json to yaml",
	"version": "0.0.6",
	"publisher": "hilleer",
	"engines": {
		"vscode": "^1.41.0"
	},
	"contributors": [
		{
			"name": "Jonatan Pedersen",
			"url": "https://www.jonatanpedersen.com/",
			"email": "jp@jonatanpedersen.com"
		},
		{
			"name": "Daniel Hillmann",
			"url": "https://github.com/hilleer",
			"email": "hiller@live.dk"
		}
	],
	"categories": [
		"Other"
	],
	"keywords": [
		"converter",
		"yaml",
		"json",
		"yaml to json",
		"json to yaml"
	],
	"activationEvents": [
		"workspaceContains:**/**.json",
		"workspaceContains:**/**.yml",
		"workspaceContains:**/**.yaml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.convertJson",

				"title": "Convert To YML"
			},
			{
				"command": "extension.convertYaml",

				"title": "Convert To JSON"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"group": "1_modification",
					"command": "extension.convertJson",
					"when": "resourceExtname == .json"
				},
				{
					"group": "1_modification",
					"command": "extension.convertYaml",
					"when": "resourceExtname == .yml || resourceExtname == .yaml"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"preversion": "npm run pretest",
		"postversion": "git push && git push --tags"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"@types/yaml": "1.2.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"i": "0.3.6",
		"npm": "6.13.7",
		"yaml": "1.7.2"
	},
	"bugs": {
		"url": "https://github.com/hilleer/vscode-yaml-plus-json/issues/new"
	},
	"repository": {
		"url": "https://github.com/hilleer/vscode-yaml-plus-json",
		"type": "git"
	}
}
